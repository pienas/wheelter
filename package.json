{
  "name": "wheelter",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "tsc && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "1.0.5",
    "@chakra-ui/react": "1.1.2",
    "@emotion/react": "11.1.4",
    "@emotion/styled": "11.0.0",
    "@nivo/core": "0.67.0",
    "@nivo/line": "0.67.0",
    "@nivo/pie": "0.67.0",
    "@prisma/client": "~2.16",
    "blitz": "0.33.0",
    "chart.js": "2.9.4",
    "cloudinary": "1.25.1",
    "date-fns": "2.17.0",
    "final-form": "4.20.1",
    "framer-motion": "3.2.0",
    "fs": "0.0.1-security",
    "prisma": "~2.16",
    "react": "0.0.0-experimental-3310209d0",
    "react-beautiful-dnd": "13.1.0",
    "react-chartjs-2": "2.11.1",
    "react-day-picker": "7.4.8",
    "react-dom": "0.0.0-experimental-3310209d0",
    "react-error-boundary": "3.1.0",
    "react-final-form": "6.5.2",
    "react-final-form-html5-validation": "1.1.0",
    "react-icons": "4.2.0",
    "react-images-uploading": "3.1.2",
    "react-phone-input-2": "2.13.9",
    "react-scrollbars-custom": "4.0.25",
    "react-slick": "0.28.1",
    "sha1": "1.1.1",
    "typescript": "4.1.5",
    "use-promise-state": "0.0.1",
    "use-sound": "2.0.1",
    "zod": "1.11.11"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.0",
    "@types/react": "17.0.2",
    "eslint": "7.20.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "prettier-plugin-prisma": "0.3.0",
    "pretty-quick": "3.1.0",
    "preview-email": "3.0.3",
    "prisma-dbml-generator": "0.4.0"
  },
  "private": true
}
